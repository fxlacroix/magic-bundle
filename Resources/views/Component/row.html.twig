<div class="row">

    {% for item in items %}
        <div class="col-sm-6 col-md-4">
            <div class="thumbnail">
                <img data-src="{{ item.picture }}" alt="100%x200" src=""
                     style="height: 200px; width: 100%; display: block;">

                <div class="caption">
                    <h3>{{ item.label }}</h3>

                    <p>{{ item.content }}</p>

                    <p><a href="#" class="btn btn-primary" role="button">Voir la gallerie</a></p>
                </div>
            </div>
        </div>
    {% endfor %}

</div>
